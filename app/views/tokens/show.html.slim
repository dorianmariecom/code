- content_for(:title, @token.to_s)

= turbo_stream_from @token

.text-sm= t(".user")
.mb-5= link_to @token.user

.text-sm= t(".id")
.mb-5= @token.id

.text-sm= t(".created_at")
.mb-5= @token.created_at

.text-sm= t(".updated_at")
.mb-5= @token.updated_at

.text-sm=> t(".token")
.mb-5= @token.token

= link_to t(".edit"), [:edit, @token]
= button_to t(".delete"), @token, method: :delete, class: "a text-red-600"
